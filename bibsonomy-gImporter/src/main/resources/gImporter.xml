<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="gImporter">
  <typeAlias alias="gImporterData" type="org.bibsonomy.gImporter.gImporterData"/>

  <resultMap id="gImporterResult" class="gImporterData">
    <result property="title" column="title" />
    <result property="year" column="pub_year_c" javaType="String"/>
    <result property="address" column="city" />
    <result property="diss" column="diss" javaType="boolean"/>
    <result property="firstName" column="firstname" />
    <result property="lastName" column="surname" />
   
  </resultMap>
	
  <select id="selectDissertation" resultMap="gImporterResult" parameterClass="gImporterData">
    select t.title, t.pub_year_c, t.city, t.diss, p.firstname, p.surname from dnb2_light.title_hs_link l join dnb2_light.title_hs_title t on l.title_id=t.title_id and l.diss=1 join dnb2_light.title_hs_person p on l.person_id=p.person_id limit 10
  </select>
  
  
</sqlMap>
