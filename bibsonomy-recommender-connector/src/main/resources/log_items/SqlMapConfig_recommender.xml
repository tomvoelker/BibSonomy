<!DOCTYPE sqlMapConfig
PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
"http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
	<!-- 
		| SQL Map Config for the recommender database
		-->

	<typeAlias alias="byteBuffer"      type="byte[]"/>	
	<typeAlias alias="timestamp"       type="java.sql.Timestamp"/>

	<typeAlias alias="recLogEntry"     type="recommender.core.database.params.RecLogEntry" />
	<typeAlias alias="recSetting"      type="recommender.core.database.params.RecSettingParam" />
	<typeAlias alias="recQuerySetting" type="recommender.core.database.params.RecQuerySettingParam" />
	<typeAlias alias="recQuery"        type="recommender.core.database.params.RecQueryParam" />
	<typeAlias alias="entityParam"     type="recommender.core.database.params.EntityParam" />
    <typeAlias alias="latencyParam"    type="recommender.core.database.params.LatencyParam" />

	<typeAlias alias="recAdminOverview" type="recommender.core.database.params.RecAdminOverview"/>

	<typeAlias alias="recResultParam" type="recommender.core.database.params.ResultParam" />

	<typeAlias alias="pair"            type="recommender.core.model.Pair" />
	<typeAlias alias="selectorMap"     type="recommender.core.database.params.SelectorQueryMapParam" />
	<typeAlias alias="selectorSetting" type="recommender.core.database.params.SelectorSettingParam" />


	<sqlMap resource="log_items/common/RecommendationLogCommon.xml" />
	<sqlMap resource="log_items/RecommendationLog.xml" />
</sqlMapConfig>


