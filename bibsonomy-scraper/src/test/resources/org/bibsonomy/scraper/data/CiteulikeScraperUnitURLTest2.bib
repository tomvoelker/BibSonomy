    number = {16},
    pages = {164706},
    posted-at = {2009-07-15 05:02:08},
    priority = {2},
    publisher = {AIP},
    title = {Molecular simulation of cooperative hydrodynamic effects in motion of a periodic array of spheres between parallel walls},
