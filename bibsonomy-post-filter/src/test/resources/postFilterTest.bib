        address = {Washington, DC, USA},
        author = {Claudiu S. Firan and Wolfgang Nejdl and Raluca Paiu},
        booktitle = {LA-WEB '07: Proceedings of the 2007 Latin American Web Conference},
        pages = {32--41},
        publisher = {IEEE Computer Society},
        year = 2007,
        url = {http://portal.acm.org/citation.cfm?id=1317537.1318430&jmp=abstract&coll=GUIDE&dl=GUIDE&CFID=66852723&CFTOKEN=79035243#abstract},
        description = {The Benefit of Using Tag-Based Profiles},
	abstract = {Collaborative tagging, i.e. the process of assigning metadata in the form of keywords to shared content by many users, has emerged as an important way to provide information about resources on the Web and elsewhere. Such keywords (tags) are used to enable the organization of information within personal information spaces, such as photo collections, but can also be shared, allowing browsing and searching with the help of tags attached by other users to information resources from the Web. Recent research has shown that such tag distributions stabilize over time and can be used to improve search on the Web. In this paper we are interested in another aspect, namely how they characterize the user and enable personalized recommendations. Using data from a frequently used music search portal, Last.fm, we analyze tag usage and statistics and investigate the use of tag-based user profiles in contrast to conventional user profiles based on song and track usage. We specify recommendation algorithms based on tag user profiles, and explore how collaborative filtering recommendations based on these tag profiles are different from recommendations based on song/track profiles. Finally, we describe a new search-based method, which uses tags to recommend songs interesting to a user, yielding substantially improved results. The paper finishes with a discussion of some future work to further improve tag-based search and recommendation in community web sites.},
	biburl = {http://www.bibsonomy.org/bibtex/223a6d3aac77330ca699f6d7464b5abb4/jaeschke},
	keywords = {bookmarking profile recommender social tag},
    isbn = {0-7695-3008-7}, doi = {http://dx.doi.org/10.1109/LA-WEB.2007.24}}

@inproceedings{benz06-automatic,
        title = {Automatic Bookmark Classification - A Collaborative Approach},
        address = {Edinburgh, Scotland},
        author = {Dominik Benz and Karen H. L. Tso and Lars Schmidt-Thieme},
        booktitle = {Proceedings of the 2nd Workshop in Innovations in Web Infrastructure (IWI2) at WWW2006},
        year = 2006,
        url = {http://www.wmin.ac.uk/~courtes/iwi2006/benz_automatic.pdf},
        
      month = May,
    abstract = {Bookmarks (or Favorites, Hotlists) are a popular strategy to relocate interesting websites on the WWW by creating a personalized local URL repository. Most current browsers offer a facility to store and manage bookmarks in a hierarchy of folders; though, with growing size, users reportedly have trouble to create and maintain a stable taxonomy. This paper presents a novel collaborative approach to ease bookmark management, especially the “classification” of new bookmarks into a folder. We propose a methodology to realize the collaborative classification idea of considering how similar users have classified a bookmark. A combination of nearest-neighbour-classifiers is used to derive a recommendation from similar users on where to store a new bookmark. Additionally, a procedure to generate keyword recommendations is proposed to ease the annotation of new bookmarks. A prototype system called CariBo has been implemented as a plugin of the central bookmark server software SiteBar. A case study conducted with real user data supports the validity of the approach.},
	biburl = {http://www.bibsonomy.org/bibtex/29d685c05008804a45b72a43586777b3b/jaeschke},
	keywords = {2006 benz bookmark bookmarking classification social tagging},
    isbn = {085432853X}}

