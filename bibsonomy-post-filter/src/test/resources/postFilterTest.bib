@inproceedings{golder2008measuring,
        title = {Measuring social networks with digital photograph collections},
        address = {New York, NY, USA},
        author = {Scott Golder},
        booktitle = {HT '08: Proceedings of the nineteenth ACM conference on Hypertext and hypermedia},
        pages = {43--48},
        publisher = {ACM},
        year = 2008,
        url = {http://portal.acm.org/citation.cfm?id=1379092.1379104},
        description = {Measuring social networks with digital photograph collections},
	abstract = {The ease and lack of cost associated with taking digital photographs have allowed people to amass large personal photograph collections. These collections contain valuable information about their owners' social relationships. This paper is a preliminary investigation into how digital photo collections can provide useful data for the study of social networks. Results from an analysis of 23 subjects' photo collections demonstrate the feasibility of this approach. The relationship between perceived closeness and network position, as well as future questions, are also discussed.},
