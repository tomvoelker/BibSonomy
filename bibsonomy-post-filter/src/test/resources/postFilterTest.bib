    location = {Napa Valley, California, USA}, isbn = {978-1-59593-991-3}, doi = {http://doi.acm.org/10.1145/1458082.1458114}}

@article{wang2008recommendations,
        title = {Recommendations based on semantically enriched museum collections},
        address = {Amsterdam, The Netherlands},
        author = {Yiwen Wang and Natalia Stash and Lora Aroyo and Peter Gorgels and Lloyd Rutledge and Guus Schreiber},
