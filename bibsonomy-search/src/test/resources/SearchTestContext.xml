<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="propertyConfigurerSearch" class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
		<property name="locations" value="classpath:search-test.properties" />
		<property name="ignoreUnresolvablePlaceholders" value="true" />
	</bean>
	
	<bean id="pdfExtractor" class="org.bibsonomy.search.testutils.DummyPDFExtractor" />
	
	<import resource="classpath:TestDatabaseBaseContext.xml" />
	
	<import resource="classpath:org/bibsonomy/bibsonomy-search-context.xml" />
	<import resource="classpath:org/bibsonomy/bibsonomy-database-common-context.xml" />
</beans>