<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="ProjectGeneration">

	<select id="getLastProjectChangeId" resultClass="int">
		SELECT value FROM ids WHERE name = #id#
	</select>

	<select id="getLastProjectChangeLogDate" resultClass="Date">
		SELECT MAX(log_date) FROM log_projects lp
	</select>
</sqlMap>