<div class="bibtex_entry">

	<div class="bibtex_header">
		<div class="bibtex_key">
			<a name="\format[AntiScriptInjection]{\bibtexkey}">\format[AntiScriptInjection]{\bibtexkey}</a>
		</div>

		\begin{pdf}
		<div class="bibtex_pdf">
			<a href="\format[ResolvePDF]{\pdf}">pdf</a>
		</div>
		\end{pdf}

		\begin{url}
		<div class="bibtex_url">
			<a href="\format[AntiScriptInjection]{\url}">url</a>
		</div>
		\end{url}
	</div>

	<div class="bibtex_content">

		\begin{title}
		<div class="bibtex_title">
			\format[HTMLChars,RemoveLatexCommands,AntiScriptInjection]{\title}
		</div>
		\end{title}

		\begin{author}
		<div class="bibtex_author">
			\format[AuthorLastFirst,AuthorAbbreviator,AuthorAndsReplacer,HTMLCharsAntiScript]{\author}
		</div>
		\end{author}

		\begin{school}
		<div class="bibtex_journal">
			\format[HTMLCharsAntiScript]{\school}
		</div>,
		\end{school}

		<div class="bibtex_year">
			\format[AntiScriptInjection]{\year}
		</div>
	</div>
</div>
