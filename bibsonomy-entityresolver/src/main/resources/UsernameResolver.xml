<duke>
	<schema>
		<threshold>0.80</threshold>
		<maybe-threshold>0.75</maybe-threshold>

		<property type="id">
			<name>user_name</name>
		</property>
		
		<property>
			<name>user_realname</name>
			<comparator>no.priv.garshol.duke.comparators.PersonNameComparator</comparator>
			<low>0.1</low>
			<high>1</high>
		</property>
		
		<property> 
			<name>place</name>
			<comparator>no.priv.garshol.duke.comparators.JaroWinklerTokenized</comparator>
			<low>0.5</low> 
			<high>0.6</high>
		</property>
		<property> 
			<name>user_homepage</name>
			<comparator>no.priv.garshol.duke.comparators.ExactComparator</comparator>
			<low>0.5</low>
			<high>0.8</high>
		</property>
	</schema>
</duke>