	<packaging>war</packaging>
	<name>BibSonomy-Webapp</name>
	<description>The web application for BibSonomy.</description>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<!-- the surefire plugin does strange things with the classpath,
					to run the webapp in the tests we need to use the forkmode never-->
				<configuration>
					<forkMode>never</forkMode>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.1.1</version>
				<configuration>
					<!-- 
						otherwise the javascript/css files compressed by
						yuicompressor-maven-plugin are overwritten by this plugin 
					-->
					<warSourceExcludes>**/*.js,**/*.css</warSourceExcludes>
				</configuration>
			</plugin>
			<plugin>
				<groupId>net.alchim31.maven</groupId>
				<artifactId>yuicompressor-maven-plugin</artifactId>
				<version>1.3.0</version>
				<executions>
					<execution>
						<id>check_and_compress</id>
						<goals>
							<goal>compress</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<excludes>
						<exclude>**/pdfworker.js</exclude>
						<exclude>**/pdf.js/**</exclude>
					</excludes>
					<encoding>UTF-8</encoding>
					<jswarn>false</jswarn>
					<nosuffix>true</nosuffix>
					<suffix></suffix>
					<disableOptimizations>true</disableOptimizations>
					<!-- note: use the compressed javascript files in ${project.build.webapp.resourcedir} -->
					<aggregations>
						<aggregation>
							<insertNewLine>true</insertNewLine>
							<output>${project.build.webapp.resourcedir}/javascript/merged/global.js</output>
							<includes>
								<include>${project.build.webapp.resourcedir}/jquery/jquery.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/functions.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/actions/clipboard.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/fileUpload.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/jquery.fadebox.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/style.js</include>
								<include>${project.build.webapp.resourcedir}/jquery/plugins/corner/jquery.corner.js</include>
								<include>${project.build.webapp.resourcedir}/jquery/plugins/hoverIntent/jquery.hoverIntent.js</include>
								<include>${project.build.webapp.resourcedir}/jquery/plugins/ui/jquery-ui.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/ajaxUtils.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/userRelation.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/addToSpheres.js</include>
							</includes>
						</aggregation>
						<aggregation>
							<insertNewLine>true</insertNewLine>
							<output>${project.build.webapp.resourcedir}/javascript/merged/discussion.js</output>
							<includes>
								<include>${project.build.webapp.resourcedir}/jquery/plugins/ui/plugins/stars/jquery.ui.stars.js</include>
								<include>${project.build.webapp.resourcedir}/jquery/plugins/flot/excanvas.js</include>
								<include>${project.build.webapp.resourcedir}/jquery/plugins/flot/jquery.flot.js</include>
								<include>${project.build.webapp.resourcedir}/jquery/plugins/flot/jquery.flot.stack.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/discussion/discussion.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/discussion/reviews.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/discussion/comments.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/discussion/csllinks.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/citeproc/csllocales.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/citeproc/xmldom.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/citeproc/citeproc.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/citeproc/cslstyles.js</include>
							</includes>
						</aggregation>
						<!-- javascript files for bootstrap front-end -->
						<aggregation>
							<insertNewLine>true</insertNewLine>
							<output>${project.build.webapp.resourcedir}/javascript/merged/bs/global.js</output>
							<includes>
								<include>${project.build.webapp.resourcedir}/javascript/bs/jquery-2.1.1.js</include>
								<include>${project.build.webapp.resourcedir}/jquery/plugins/ui/jquery-ui.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/bootstrap.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/bootstrap-datepicker.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/bootstrap-carousel.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/bootstrap-tagsinput.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/bootstrap-dialog.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/holder.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/custom.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/friendoverview.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/functions.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/actions/clipboard.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/ajaxUtils.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/fileUpload.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/style.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/jquery.fadebox.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/userRelation.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/addToSpheres.js</include>
							</includes>
						</aggregation>
						<aggregation>
							<insertNewLine>true</insertNewLine>
							<output>${project.build.webapp.resourcedir}/javascript/merged/bs/discussion.js</output>
							<includes>
								<include>${project.build.webapp.resourcedir}/jquery/plugins/ui/plugins/stars/jquery.ui.stars.js</include>
								<include>${project.build.webapp.resourcedir}/jquery/plugins/flot/excanvas.js</include>
								<include>${project.build.webapp.resourcedir}/jquery/plugins/flot/jquery.flot.js</include>
								<include>${project.build.webapp.resourcedir}/jquery/plugins/flot/jquery.flot.stack.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/discussion/discussion.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/discussion/reviews.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/discussion/comments.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/bs/discussion/csllinks.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/citeproc/csllocales.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/citeproc/xmldom.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/citeproc/citeproc.js</include>
								<include>${project.build.webapp.resourcedir}/javascript/citeproc/cslstyles.js</include>
							</includes>
						</aggregation>
					</aggregations>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.lesscss</groupId>
				<artifactId>lesscss-maven-plugin</artifactId>
				<version>1.7.0.1.1</version>
				<configuration>
					<sourceDirectory>${project.basedir}/src/main/webapp/resources/css</sourceDirectory>
					<outputDirectory>${project.build.directory}/${project.build.finalName}/resources/css</outputDirectory>
					<compress>true</compress>
					<includes>
						<include>bootstrap.less</include>
						<include>bootstrap-style.less</include>
						<include>style.less</include>
					</includes>
				</configuration>
				<executions>
					<execution>
						<goals>
							<goal>compile</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>
		<testResources>
			<testResource>
				<directory>src/test/resources</directory>
			</testResource>
			<testResource>
				<directory>src/main/webapp</directory>
			</testResource>
		</testResources>
		<pluginManagement>
			<plugins>
				<!--This plugin's configuration is used to store Eclipse m2e settings only. It has no influence on the Maven build itself.-->
				<plugin>
					<groupId>org.eclipse.m2e</groupId>
					<artifactId>lifecycle-mapping</artifactId>
					<version>1.0.0</version>
					<configuration>
						<lifecycleMappingMetadata>
							<pluginExecutions>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>org.apache.maven.plugins</groupId>
										<artifactId>maven-antrun-plugin</artifactId>
										<versionRange>[1.4,)</versionRange>
										<goals>
											<goal>run</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore></ignore>
									</action>
								</pluginExecution>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>net.alchim31.maven</groupId>
										<artifactId>yuicompressor-maven-plugin</artifactId>
										<versionRange>[1.1,)</versionRange>
										<goals>
											<goal>compress</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore></ignore>
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>
