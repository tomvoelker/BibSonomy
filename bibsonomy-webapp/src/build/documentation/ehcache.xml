<!-- Copy this file to WEB-INF/classes -->
<ehcache>
   <diskStore path="java.io.tmpdir"/>
   <defaultCache
    maxElementsInMemory="1"
    eternal="false"
    overflowToDisk="true"
    timeToIdleSeconds="1"
    timeToLiveSeconds="1"
    diskPersistent="false"
    diskExpiryThreadIntervalSeconds="1"/>
   <cache
    name="org.apache.slide.webdav.util.UriHandler"
    maxElementsInMemory="1"
    eternal="false"
    overflowToDisk="true"
    timeToIdleSeconds="1"
    timeToLiveSeconds="1"
    diskPersistent="false"
    diskExpiryThreadIntervalSeconds="1"/>
</ehcache>
