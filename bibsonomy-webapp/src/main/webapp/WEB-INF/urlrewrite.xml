    <!-- NEW page -->
    <rule>
    	<condition type="attribute" name="type" operator="notequal"/>
        <from>^/+basket(\?(.*))?$</from>
        <to>/basket?$2</to>
    </rule>	
    <rule>
        <from>^/+basket(\?(.*))?$</from>
        <to>/ResourceHandler?page=basket&amp;$2</to>
    </rule>

    

	<!-- ############## /my* mappings ############### -->
	<!-- 
		currently we handle the following mappings:
		/myBibSonomy
		/myBibTeX
		/myRelations
		/myPDF
		/myDuplicates
	 -->
	<!-- FIXME: it is very optimistic, to send ALL URLs starting with /my to there ... -->
	<rule>
		<from>^/(my.+)(\?(.*))?$</from>
		<to>/redirect?myPage=$1&amp;$3</to>
	</rule>
    <!-- ############## content negotiation ############### -->
	<rule>	
		<from>^/+uri/+(.+)$</from>
		<to last="true">/redirect?url=$1</to>
	</rule>	

	<!-- ############## new actions - testing! ############### -->
	<rule><!-- /actions/admin/group/GROUPNAME -->
		<from>^/+actions/+admin/+group/+([^/?]+?)(\?(.*))?$</from>
		<to>/actions/admin/group?requestedGroup=$1&amp;$3</to>
	</rule>


    <!-- ############## old pages - can be removed when never accessed ############### -->
    <rule><!-- /faq to /help/faq -->
      <from>^/faq(.*)$</from>
      <to type="permanent-redirect">/help/faq/$1</to>
    </rule>

	<outbound-rule>
        <note>
            The outbound-rule specifies that when response.encodeURL is called (if you are using JSTL c:url)
            the url /rewrite-status will be rewritten to /test/status/.

            The above rule and this outbound-rule means that end users should never see the
            url /rewrite-status only /test/status/ both in thier location bar and in hyperlinks
            in your pages.
        </note>
        <from>/rewrite-status</from>
        <to>/test/status/</to>
    </outbound-rule>

</urlrewrite>
