<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="RepositoryCommon">
	
	<resultMap id="repository" class="repository" groupBy="repository_name" >
		<result property="id" 		column="repository_name" 	javaType="string" />
		<result property="date"  	column="date"				javaType="date" />
	</resultMap>
	

	<sql id="allRepositoryAttributes">
		r.repository_name, r.date 
	</sql>
	
</sqlMap>