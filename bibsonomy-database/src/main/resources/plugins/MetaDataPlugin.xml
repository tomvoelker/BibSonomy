<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="MetaDataPlugin">
	
	<insert id="logPostCopy" parameterClass="metaDataPostParam">
		INSERT INTO post_metadata (`user_name`, `intra_hash`, `inter_hash`, `key`, `value`)
		VALUES (#userName#, #intraHash#, #interHash#, #key#, #value#)
	</insert>

</sqlMap>