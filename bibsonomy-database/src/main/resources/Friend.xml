<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="Friend">
	<select id="getUserFriends" resultMap="FriendCommon.friendName"
		parameterClass="string">
		SELECT f_user_name FROM friends WHERE user_name = #username#
	</select>
	
	<select id="getFriendsOfUserByStringAsStrings" resultMap="FriendCommon.userName"
		parameterClass="string">
		SELECT user_name FROM friends WHERE f_user_name = #username#
	</select>
</sqlMap>